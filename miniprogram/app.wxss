.plus-icon.bordered::before {
  top: calc(var(--plus-icon-bordered-width-half) - var(--plus-icon-bordered-height-half));
  left: calc(var(--plus-icon-bordered-width-half) - var(--plus-icon-bordered-inner-width-half));
  width: var(--plus-icon-bordered-inner-width);
  height: var(--plus-icon-bordered-height);
